{
  "name": "ace-remodeling-app",
  "main": "expo-router/entry",
  "version": "2.1.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "dev:clear": "expo start --dev-client --clear",
    "dev:ios": "expo run:ios",
    "dev:android": "expo run:android",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "lint": "expo lint",
    "extract-projects": "tsx scripts/utilities/extractProjectList.ts",
    "find-invalid-assets": "tsx scripts/utilities/findInvalidAssets.ts",
    "build:ios": "eas build --platform ios --profile production",
    "build:ios:submit": "eas build --platform ios --profile production --auto-submit",
    "build:preview": "eas build --platform ios --profile preview",
    "build:status": "eas build:list",
    "deploy:update": "eas update --branch production --platform ios",
    "deploy:full": "eas build --platform ios --profile production --auto-submit",
    "upload": "tsx scripts/uploadProjects.ts",
    "upload:dev": "NODE_ENV=development tsx scripts/uploadProjects.ts",
    "upload:prod": "NODE_ENV=production tsx scripts/uploadProjects.ts",
    "add-featured:dev": "tsx scripts/utilities/addFeaturedField.ts --env=dev",
    "add-featured:prod": "tsx scripts/utilities/addFeaturedField.ts --env=prod",
    "legacy:upload": "tsx scripts/legacy/uploadPhotos.ts",
    "legacy:upload:prod": "NODE_ENV=production tsx scripts/legacy/uploadPhotos.ts",
    "legacy:seed": "tsx scripts/legacy/seedFirebase.ts",
    "legacy:seed:prod": "NODE_ENV=production tsx scripts/legacy/seedFirebase.ts",
    "version:patch": "npm version patch && node scripts/sync-version.js",
    "version:minor": "npm version minor && node scripts/sync-version.js",
    "version:major": "npm version major && node scripts/sync-version.js",
    "version:info": "echo 'Current version:' && grep '\"version\":' package.json",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/metro-config": "^54.0.3",
    "@expo/metro-runtime": "^6.1.1",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "1.24.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "babel-preset-expo": "^54.0.1",
    "expo": "~54.0.3",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.8",
    "expo-file-system": "^19.0.17",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.1",
    "expo-splash-screen": "~31.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.7",
    "firebase": "^12.4.0",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.81.5",
    "react-native-blob-util": "^0.22.2",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pdf": "^7.0.3",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "use-debounce": "^10.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@config-plugins/react-native-blob-util": "^12.0.0",
    "@config-plugins/react-native-pdf": "^12.0.0",
    "@types/jest": "^30.0.0",
    "@types/papaparse": "^5.5.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "private": true
}
