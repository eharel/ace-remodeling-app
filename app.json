{
  "expo": {
    "name": "ACE Remodeling TX",
    "slug": "ace-remodeling-app",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "aceremodelingapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aceremodelingtx.app",
      "buildNumber": "9",
      "infoPlist": {
        "CFBundleDisplayName": "ACE Remodeling TX"
      }
    },
    "android": {
      "displayName": "ACE Remodeling TX",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.DOWNLOAD_WITHOUT_NOTIFICATION",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "com.aceremodelingtx.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/ccb81db1-d023-4ef7-bcb9-b8c536378d91"
    },
    "runtimeVersion": "1.0.0",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ccb81db1-d023-4ef7-bcb9-b8c536378d91"
      }
    }
  }
}
